{
  "name": "review",
  "version": "1.0.001",
  "author": "Make: Community",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "cachebust:update": "npm version patch",
    "postcachebust:update": "npm run replace:version",
    "replace:version": "replace-in-file '/Version\\: [0-9]\\.{0,1}[0-9]*\\.{0,1}[0-9]*/' 'Version: '\"$npm_package_version\" style.css --isRegex",
    "onchange:js:vue": "onchange 'js/*.js' -- npm run js:vue:uglify",
    "js:vue:uglify": "uglifyjs js/*.js node_modules/vue/dist/vue.js node_modules/bootstrap-vue/dist/bootstrap-vue.js node_modules/axios/dist/axios.js -o js/min/review.min.js -c -m --source-map \"root='../', url='review.min.js.map'\"",
    "build": "npm-run-all js:**:uglify",
    "build:cachebust": "npm-run-all cachebust:update js:**:uglify",
    "dev": "npm-run-all build -p onchange:js:**"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bootstrap": "~4.6.1",
    "bootstrap-vue": ">=2.23.1",
    "vue": "~2.6.12",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "replace-in-file": "^3.4.4",
    "uglify-js": "^3.7.1"
  }
}
