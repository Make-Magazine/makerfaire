{
   "name": "resource-mgmt",
   "version": "1.00.000",
   "description": "Resource Management",
   "main": "",
   "private": true,
   "repository": {
      "type": "git",
      "url": "https://github.com/Make-Magazine/makerfaire.git"
   },
   "scripts": {
      "cachebust:update": "npm version patch",
      "postcachebust:update": "npm run replace:version",
      "replace:version": "replace-in-file '/Version\\: [0-9]\\.{0,1}[0-9]*\\.{0,1}[0-9]*/' 'Version: '\"$npm_package_version\" style.css --isRegex",
      "scss:bs:compile": "sass scss/bootstrap/bootstrap.scss css/bootstrap.min.css --style compressed",
      "scss:ar:compile": "sass scss/angular-reporting-style.scss css/angular-reporting.min.css --style compressed",
      "concat:kendo:css": "concat -o lib/Kendo/kendo.min.css lib/Kendo/content/css/web/kendo.common-material.min.css lib/Kendo/content/css/web/kendo.rtl.min.css lib/Kendo/content/css/web/kendo.material.min.css lib/Kendo/content/css/web/kendo.material.mobile.min.css lib/Kendo/content/css/dataviz/kendo.dataviz.min.css lib/Kendo/content/css/dataviz/kendo.dataviz.default.min.css lib/Kendo/styles/examples.css lib/Kendo/woahbar/woahbar.css",
      "js:ngreporting:uglify": "uglifyjs js/*.js js/grunt-scripts/*.js node_modules/bootstrap/dist/js/bootstrap.min.js node_modules/bootstrap-dialog/dist/js/bootstrap-dialog.min.js node_modules/angular/angular.js node_modules/angular-route/angular-route.js node_modules/angular-ui-bootstrap/dist/ui-bootstrap-tpls.js node_modules/angular-utils-pagination/dirPagination.js node_modules/angular-animate/angular-animate.js node_modules/angular-sanitize/angular-sanitize.js node_modules/angular-ui-grid/ui-grid.min.js -o js/built-angular-reporting.min.js node_modules/angular-filter/dist/angular-filter.js  -c -m --source-map \"root='../', url='built-angular-reporting.min.js.map'\"",
      "onchange:scss:bs": "onchange 'scss/bootstrap/*.scss' -- npm run scss:bs:compile",
      "onchange:scss:ar": "onchange 'scss/*.scss' -- npm run scss:ar:compile",
      "onchange:js:nglibs": "onchange 'js/*.js' -- npm run js:nglibs:uglify",
      "build": "npm-run-all js:**:uglify scss:**:compile concat:**:css",
      "build:cachebust": "npm-run-all cachebust:update js:**:uglify scss:**:compile concat:**:css",
      "dev": "npm-run-all build -p onchange:js:** onchange:scss:**"
   },
   "devDependencies": {
      "@babel/cli": "^7.14.8",
      "@babel/core": "^7.9.6",
      "@babel/preset-env": "^7.9.6",
      "bootstrap": "^3.4.1",
      "bootstrap-dialog": "1.34.6",
      "clean-css-cli": "^4.3.0",
      "npm-run-all": "^4.1.5",
      "onchange": "^7.1.0",
      "replace-in-file": "^3.4.4",
      "uglify-js": "^3.9.2"
   },
   "author": "Make: Community",
   "license": "UNLICENSED",
   "dependencies": {
      "@fancyapps/fancybox": "^3.5.7",
      "angular": "^1.6.10",
      "angular-animate": "1.7.2",
      "angular-filter": "0.5.17",
      "angular-route": "1.7.2",
      "angular-sanitize": "1.7.2",
      "angular-ui-bootstrap": "2.5.6",
      "angular-ui-grid": "^4.11.0",
      "angular-utils-pagination": "0.11.1",
      "bootstrap-sass": "^3.4.3",
      "concat": "^1.0.3",
      "sass": "^1.77.0"
   }
}
