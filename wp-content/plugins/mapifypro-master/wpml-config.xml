<wpml-config>
  	<custom-types>
		<custom-type translate="1">map</custom-type>
		<custom-type translate="1">map-location</custom-type>
		<custom-type translate="1">map-drawer</custom-type>
		<custom-type translate="1">route</custom-type>
  	</custom-types>
	<taxonomies>
		<taxonomy translate="1">location-tag</taxonomy>
	</taxonomies>
  	<custom-fields>
		<!-- Map: Labels Settings -->
		<custom-field action="translate" label="Directions Button Text">_map_label_directions_button</custom-field>
		<custom-field action="translate" label="Location Details Text">_map_label_location_details</custom-field>
		<custom-field action="translate" label="Filter Dropdown Text">_map_label_filter_dropdown</custom-field>
		<custom-field action="translate" label="Filter List Text">_map_label_filter_list</custom-field>
		<custom-field action="translate" label="First Label Search">_map_label_search</custom-field>
		<custom-field action="translate" label="Second Label Search">_map_label_search_second</custom-field>
		<custom-field action="translate" label="Search Within Text">_map_label_search_within</custom-field>
		<custom-field action="translate" label="No Search Results: Line 1">_map_label_no_search_results_line_1</custom-field>
		<custom-field action="translate" label="No Search Results: Line 2">_map_label_no_search_results_line_2</custom-field>
		<custom-field action="translate" label="No Search Results With Closest Location: Line 1">_map_label_no_search_results_with_closest_line_1</custom-field>
		<custom-field action="translate" label="No Search Results With Closest Location: Line 2">_map_label_no_search_results_with_closest_line_2</custom-field>
		<custom-field action="translate" label="No Search Results With Closest Location: Line 3">_map_label_no_search_results_with_closest_line_3</custom-field>
		<custom-field action="translate" label="Search Geolocation Failure: Line 1">_map_label_search_geolocation_failure_line_1</custom-field>
		<custom-field action="translate" label="Search Geolocation Failure: Line 2">_map_label_search_geolocation_failure_line_2</custom-field>
		<custom-field action="translate" label="Open Pop-up Button Label">_map_label_interactive_list_open_popup_button</custom-field>
		<custom-field action="translate" label="Directions Button Label">_map_label_interactive_list_directions_button</custom-field>

		<!-- Map: Basic Settings -->
		<custom-field action="copy">_map_mode</custom-field>
		<custom-field action="copy">_map_pin</custom-field>
		<custom-field action="copy">_map_pins_hide_initially</custom-field>
		<custom-field action="copy">_map_enable_zoom</custom-field>
		<custom-field action="copy">_map_enable_zoom_manual</custom-field>
		<custom-field action="copy">_map_enable_clustering</custom-field>
		<custom-field action="copy">_map_enable_use_my_location</custom-field>

		<!-- Map: Search & Filter Settings -->
		<custom-field action="copy">_map_enable_search</custom-field>
		<custom-field action="copy">_map_enable_filters</custom-field>
		<custom-field action="copy">_map_enable_filters_list</custom-field>
		<custom-field action="copy">_map_search_center</custom-field>
		<custom-field action="copy">_map_filters_center</custom-field>

		<!-- Map: Search Radius Settings -->
		<custom-field action="copy">_map_search_radius</custom-field>
		<custom-field action="copy">_map_search_radius_unit</custom-field>
		<custom-field action="copy">_map_search_region_bias</custom-field>
		<custom-field action="copy">_map_enable_search_radius</custom-field>

		<!-- Map: Tooltip & Marker / Pinpoint Settings -->
		<custom-field action="copy">_map_animate_tooltips</custom-field>
		<custom-field action="copy">_map_tooltip_image_orientation</custom-field>
		<custom-field action="copy">_map_animate_pinpoints</custom-field>

		<!-- Map: Interactive List of Locations Settings -->
		<custom-field action="copy">_map_mll_include</custom-field>
		<custom-field action="copy">_map_mll_number_of_locations</custom-field>
		<custom-field action="copy">_map_mll_hide_in_default_view</custom-field>
		<custom-field action="copy">_map_label_interactive_list_open_popup_bgcolor</custom-field>
		<custom-field action="copy">_map_label_interactive_list_open_popup_color</custom-field>
		<custom-field action="copy">_map_label_interactive_list_directions_bgcolor</custom-field>
		<custom-field action="copy">_map_label_interactive_list_directions_color</custom-field>
		<custom-field action="copy">_map_label_interactive_list_buttons_hover_bgcolor</custom-field>
		<custom-field action="copy">_map_label_interactive_list_buttons_hover_color</custom-field>

		<!-- Map: Color / Design Settings -->
		<custom-field action="copy">_map_background_color</custom-field>
		<custom-field action="copy">_map_tooltip_background_color</custom-field>
		<custom-field action="copy">_map_tooltip_background_transparency</custom-field>
		<custom-field action="copy">_map_tooltip_text_color</custom-field>
		<custom-field action="copy">_map_popup_background_color</custom-field>
		<custom-field action="copy">_map_popup_header_background_color</custom-field>
		<custom-field action="copy">_map_popup_date_background_color</custom-field>
		<custom-field action="copy">_map_popup_accent_color</custom-field>

		<!-- Map: Map Mode Settings -->
		<custom-field action="copy">_map_google_mode</custom-field>
		
		<!-- Map: Image Mode Settings -->
		<custom-field action="copy">_map_image_big</custom-field>
		<custom-field action="copy">osm_maptiles_status</custom-field>
		<custom-field action="copy">mapify_map_tiles_uploader</custom-field>
		<custom-field action="copy">osm_maptiles_image_url</custom-field>
		<custom-field action="copy">osm_maptiles_download_folder_name</custom-field>
		<custom-field action="copy">osm_maptiles_job_id</custom-field>
		<custom-field action="copy">osm_maptiles_tiles_data</custom-field>
		<custom-field action="copy">osm_maptiles_downloaded_tiles_index</custom-field>
		
		<!-- Map: Default Zoom Level and Location Settings -->
		<custom-field action="copy">_map_main_location</custom-field>
		<custom-field action="copy">_map_google_center-lat</custom-field>
		<custom-field action="copy">_map_google_center-lng</custom-field>
		<custom-field action="copy">_map_google_center-zoom</custom-field>

		<!-- Map Location: Labels Settings -->
		<custom-field action="translate" label="Tooltip" style="textarea">_map_location_tooltip</custom-field>
		<custom-field action="translate" label="Short Description" style="textarea">_map_location_mll_description</custom-field>

		<!-- Map Location: General Settings -->
		<custom-field action="copy">_map_location_tooltip_enabled</custom-field>
		<custom-field action="copy">_map_location_external_url_enable</custom-field>
		<custom-field action="copy">_map_location_external_url_url</custom-field>
		<custom-field action="copy">_map_location_external_url_target</custom-field>
		<custom-field action="copy">_map_location_tooltip_show</custom-field>
		<custom-field action="copy">_map_location_tooltip_close</custom-field>
		<custom-field action="copy">_map_location_popup_location_information</custom-field>
		<custom-field action="copy">_map_location_popup_directions</custom-field>
		<custom-field action="copy">_map_location_mll_include</custom-field>
		<custom-field action="copy">_map_location_pin</custom-field>

		<!-- Map Location: Address Details -->
		<custom-field action="copy">_map_location_phone</custom-field>
		<custom-field action="copy">_map_location_address</custom-field>
		<custom-field action="copy">_map_location_address_2</custom-field>
		<custom-field action="copy">_map_location_city</custom-field>
		<custom-field action="copy">_map_location_state</custom-field>
		<custom-field action="copy">_map_location_zip</custom-field>
		<custom-field action="copy">_map_location_country</custom-field>
		<custom-field action="copy">_map_location_directions_url</custom-field>

		<!-- Map Location: Share Buttons -->
		<custom-field action="copy">_map_location_share</custom-field>
		
		<!-- Map Location: Google Rich Results -->
		<custom-field action="copy">mapify_grs_enable</custom-field>
		<custom-field action="copy">mapify_grs_location_type</custom-field>
		<custom-field action="copy">mapify_grs_site_url</custom-field>
		<custom-field action="copy">mapify_grs_price_range</custom-field>

		<!-- Map Location: Gallery Settings -->
		<custom-field action="copy">_map_location_video_embed</custom-field>
		
		<!-- Map Location: Map Location -->
		<custom-field action="copy">_map_location_google_location-lat</custom-field>
		<custom-field action="copy">_map_location_google_location-lng</custom-field>
		<custom-field action="copy">_map_location_google_location-zoom</custom-field>
		<custom-field action="copy">_map_location_google_location</custom-field>

		<!-- Map Area Drawer: Labels Settings -->
		<custom-field action="translate" label="Description" style="textarea">map_drawer_description</custom-field>

		<!-- Map Area Drawer: General Settings -->
		<custom-field action="copy">_map_location_google_location</custom-field>
		<custom-field action="copy">_map_location_google_location-lat</custom-field>
		<custom-field action="copy">_map_location_google_location-lng</custom-field>
		<custom-field action="copy">_map_location_google_location-zoom</custom-field>
		<custom-field action="copy">mpfy_area_coordinates</custom-field>
		<custom-field action="copy">map_drawer_border_color</custom-field>
		<custom-field action="copy">map_drawer_fill_color</custom-field>
		<custom-field action="copy">map_drawer_fill_opacity_percentage</custom-field>
		<custom-field action="copy">map_drawer_image</custom-field>

		<!-- Route: General Settings -->
		<custom-field action="copy">_route_type</custom-field>
		<custom-field action="copy">_route_color</custom-field>

		<!-- Route: Map Route -->
		<custom-field action="copy">_route_route</custom-field>
		<custom-field action="copy">_route_route-lat</custom-field>
		<custom-field action="copy">_route_route-lng</custom-field>
		<custom-field action="copy">_route_route-zoom</custom-field>

		<!-- Route: From (origin) -->
		<custom-field action="copy">_route_origin_pin_enabled</custom-field>
		<custom-field action="copy">_route_origin_tooltip_enabled</custom-field>
		<custom-field action="copy">_route_origin_tooltip_close</custom-field>
		<custom-field action="translate" label="From (origin) pin tooltip">_route_origin_tooltip_content</custom-field>
		<custom-field action="copy">_route_origin_pin</custom-field>

		<!-- Route: Waypoints -->
		<custom-field action="translate">acf_prettyroutes_waypoints</custom-field>
		
		<!-- Route: To (destination) -->
		<custom-field action="copy">_route_destination_pin_enabled</custom-field>
		<custom-field action="copy">_route_destination_tooltip_enabled</custom-field>
		<custom-field action="copy">_route_waypoint_destination_tooltip_close</custom-field>
		<custom-field action="translate" label="To (destination) pin tooltip">_route_destination_tooltip_content</custom-field>
		<custom-field action="copy">_route_destination_pin</custom-field>

		<!-- CrowdMaps: Map Fields -->
		<custom-field action="copy">_map_crowd_enabled</custom-field>
		<custom-field action="copy">_map_crowd_require_approval</custom-field>
		<custom-field action="translate" label="CrowdMaps Thank You Popup Title">_map_crowd_thank_you_title</custom-field>
		<custom-field action="translate" label="CrowdMaps Thank You Popup Description" style="textarea">_map_crowd_thank_you_content</custom-field>

		<!-- CrowdMaps: Map Location Fields -->
		<custom-field action="copy">mpfy_contributor</custom-field>
  	</custom-fields>
	<custom-fields-texts>
		<key name="acf_prettyroutes_waypoints">
			<key name="*">
				<key name="tooltip" />
			</key>
		</key>
	</custom-fields-texts>
</wpml-config>