{
   "name": "makerfaire",
   "version": "1.0.0",
   "description": "Maker Faire",
   "main": "Gruntfile.js",
   "private": true,
   "scripts": {
      "less:bs:compile": "lessc --source-map-less-inline --source-map-map-inline --line-numbers=comments less/bootstrap/bootstrap.less css/bootstrap.css",
      "postless:bs:compile": "npm run less:bs:minify",
      "less:bs:minify": "cleancss -o css/bootstrap.min.css css/bootstrap.css --source-map",
      "postless:bs:minify": "rm css/bootstrap.css",

      "less:mf:compile": "lessc --source-map-less-inline --source-map-map-inline --line-numbers=comments less/style.less css/style.css",
      "postless:mf:compile": "npm run less:mf:minify",
      "less:mf:minify": "cleancss -o css/style.min.css css/style.css --source-map",
      "postless:mf:minify": "rm css/style.css",

      "less:ar:compile": "lessc --source-map-less-inline --source-map-map-inline --line-numbers=comments less/angular-reporting-style.less css/angular-reporting.css",
      "postless:ar:compile": "npm run less:ar:minify",
      "less:ar:minify": "cleancss -o css/angular-reporting.min.css css/angular-reporting.css --source-map",
      "postless:ar:minify": "rm css/angular-reporting.css",

      "less:mfadmin:compile": "lessc --source-map-less-inline --source-map-map-inline --line-numbers=comments less/mf-admin-style.less css/mf-admin-style.css",
      "postless:mfadmin:compile": "npm run less:mfadmin:minify",
      "less:mfadmin:minify": "cleancss -o css/mf-admin-style.min.css css/mf-admin-style.css --source-map",
      "postless:mfadmin:minify": "rm css/mf-admin-style.css",

      "less:portfolio:compile": "lessc --source-map-less-inline --source-map-map-inline --line-numbers=comments less/portfolio-styles/portfolio.less css/portfolio-style.css",
      "postless:portfolio:compile": "npm run less:portfolio:minify",
      "less:portfolio:minify": "cleancss -o css/portfolio-style.min.css css/portfolio-style.css --source-map",
      "postless:portfolio:minify": "rm css/portfolio-style.css"
   },
   "devDependencies": {
      "bootstrap": "3.3.7",
      "bootstrap-dialog": "1.34.6",
      "clean-css-cli": "^4.2.1",
      "fancybox": "3.0.1",
      "grunt": "^1.0.1",
      "grunt-contrib-concat": "1.0.1",
      "grunt-contrib-less": "^2.0.0",
      "grunt-contrib-uglify": "^3.3.0",
      "grunt-contrib-watch": "^1.1.0",
      "grunt-text-replace": "0.4.0",
      "hammerjs": "2.0.8",
      "jquery": "2.2.4",
      "jquery.cookie": "1.4.1",
      "less": "^3.9.0"
   },
   "author": "Maker Media",
   "license": "UNLICENSED",
   "dependencies": {
      "@fancyapps/fancybox": "^3.3.5",
      "angular": "^1.7.2",
      "angular-animate": "1.7.2",
      "angular-filter": "0.5.17",
      "angular-route": "1.7.2",
      "angular-sanitize": "1.7.2",
      "angular-ui-bootstrap": "2.5.6",
      "angular-ui-grid": "4.6.0",
      "angular-utils-pagination": "0.11.1"
   }
}