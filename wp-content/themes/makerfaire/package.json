{
   "name": "makerfaire",
   "version": "4.33.913",
   "description": "Maker Faire",
   "main": "",
   "private": true,
   "repository": {
      "type": "git",
      "url": "https://github.com/Make-Magazine/makerfaire.git"
   },
   "scripts": {
      "cachebust:update": "npm version patch",
      "postcachebust:update": "npm run replace:version",
      "replace:version": "replace-in-file '/Version\\: [0-9]\\.{0,1}[0-9]*\\.{0,1}[0-9]*/' 'Version: '\"$npm_package_version\" style.css --isRegex",
      "scss:mf:compile": "sass scss/style.scss css/style.min.css --style compressed",
      "scss:gf:compile": "sass scss/gravity/gravity-style.scss css/gravity-style.min.css --style compressed",
      "scss:mfadmin:compile": "sass scss/admin-styles/mf-admin-style.scss css/mf-admin-style.min.css --style compressed",
      "js:uglify": "uglifyjs node_modules/bootstrap/dist/js/bootstrap.min.js node_modules/bootstrap-dialog/dist/js/bootstrap-dialog.min.js node_modules/jquery.cookie/jquery.cookie.js node_modules/fancybox/dist/js/jquery.fancybox.pack.js js/allsite/*.js -o js/built.min.js -c -m --source-map \"root='../', url='built.min.js.map'\"",
      "js:nglibs:uglify": "uglifyjs node_modules/angular/angular.js node_modules/angular-route/angular-route.js node_modules/angular-ui-bootstrap/dist/ui-bootstrap-tpls.js node_modules/angular-utils-pagination/dirPagination.js node_modules/angular-animate/angular-animate.js node_modules/angular-sanitize/angular-sanitize.js node_modules/angular-filter/dist/angular-filter.js js/angular/schedule_cont.js js/angular/meet-the-makers.js -o js/built-angular-libs.min.js -c --source-map \"root='../', url='built-angular-libs.min.js.map'\"",
      "js:admin:uglify": "uglifyjs  js/admin/*.js -o js/built-admin-scripts.min.js -c -m --source-map \"root='../', url='built-admin-scripts.min.js.map'\"",
      "onchange:scss:mf": "onchange 'scss/*.scss' -- npm run scss:mf:compile",
      "onchange:scss:gf": "onchange 'scss/gravity/*.scss' -- npm run scss:gf:compile",
      "onchange:scss:mfadmin": "onchange 'scss/admin-styles/*.scss' 'scss/admin-styles/mf-admin-style.scss' -- npm run scss:mfadmin:compile",
      "onchange:js:allsite": "onchange 'js/allsite/*.js' -- npm run js:uglify",
      "onchange:js:nglibs": "onchange 'js/angular/*.js' -- npm run js:nglibs:uglify",
      "onchange:js:admin": "onchange 'js/admin/*.js' -- npm run js:admin:uglify",
      "js:transpile:map": "babel js/mf-map/mf-map-src.js -d js/mf-map/mf-map-src-es5 --presets=@babel/env",
      "postjs:transpile:map": "npm run js:map:uglify",
      "js:map:uglify": "uglifyjs js/mf-map/mf-map-src-es5/mf-map-src.js -o js/mf-map/min/mf-map.min.js -c -m --source-map \"root='../../../', url='mf-map.min.js.map'\"",
      "onchange:vue:map": "onchange './js/mf-map/mf-map-src.js' -- npm run js:transpile:map",
      "onchange:js:vue": "onchange 'js/vue/*.js' -- npm run js:vue:uglify",
      "js:vue:uglify": "uglifyjs js/vue/*.js -o js/min/maker-portal.min.js -c -m --source-map \"root='../../', url='vue.min.js.map'\"",
      "build": "npm-run-all js:**:uglify js:transpile:map scss:**:compile",
      "build:cachebust": "npm-run-all cachebust:update js:**:uglify js:transpile:map scss:**:compile",
      "dev": "npm-run-all build -p onchange:js:** onchange:vue:map onchange:scss:**"
   },
   "devDependencies": {
      "@babel/cli": "^7.14.8",
      "@babel/core": "^7.9.6",
      "@babel/preset-env": "^7.9.6",
      "bootstrap": "3.3.7",
      "bootstrap-dialog": "1.34.6",
      "clean-css-cli": "^4.3.0",
      "fancybox": "3.0.1",
      "jquery.cookie": "1.4.1",
      "less": "^3.11.1",
      "npm-run-all": "^4.1.5",
      "onchange": "^5.2.0",
      "replace-in-file": "^3.4.4",
      "uglify-js": "^3.9.2"
   },
   "author": "Make: Community",
   "license": "UNLICENSED",
   "dependencies": {
      "@fancyapps/fancybox": "^3.5.7",
      "angular": "^1.7.9",
      "angular-animate": "1.7.2",
      "angular-filter": "0.5.17",
      "angular-route": "^1.7.2",
      "angular-sanitize": "1.7.2",
      "angular-ui-bootstrap": "2.5.6",
      "angular-utils-pagination": "^0.11.1",
      "bootstrap-sass": "^3.4.3",
      "concat": "^1.0.3",
      "sass": "^1.77.0"
   }
}
